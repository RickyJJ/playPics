aths.push(new SayingVO(\"骑着自行车去\", new Point(50, 50), \"test/bike.jpg\", true));\r\n        this.paths.push(new SayingVO(\"骑着自行车兜风阿！\", new Point(50, 50), \"test/bike2.jpeg\", true));\r\n        this.paths.push(new SayingVO(\"让我把你教会吧\", new Point(50, 50), \"test/bike3.jpg\", true));\r\n    }\r\n    onEnable() {\r\n        this._control = this.getComponent(GameControl);\r\n        this.pics.on(Event.CLICK, this, this.onPicsClick);\r\n    }\r\n    onPicsClick(e) {\r\n        if (this.sayingIndex >= this.paths.length) {\r\n            this.sayingIndex = 0;\r\n        }\r\n        let sayingVO = this.paths[this.sayingIndex];\r\n        if (sayingVO.isTogglePic) {\r\n            this.pics.graphics.clear();\r\n            this.pics.loadImage(sayingVO.picPath);\r\n        }\r\n        this.question.text = sayingVO.text;\r\n        this.question.x = sayingVO.point.x;\r\n        this.question.y = sayingVO.point.y;\r\n        this.question.fontSize = 60;\r\n        this.question.color = '#ffffff';\r\n        this.sayingIndex++;\r\n    }\r\n}\r\n",
